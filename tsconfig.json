{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@screens/*": [
        "./app/screens/*"
      ],
      "@redux/*": [
        "./app/redux/*"
      ],
      "@components/*": [
        "./app/components/*"
      ],
      "@utils/*": [
        "./app/utils/*"
      ],      
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
